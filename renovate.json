{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  "prHourlyLimit": 2,
  "prConcurrentLimit": 6,
  "packageRules": [
    {
      "matchPackageNames": "net.sourceforge.plantuml:plantuml",
      "allowedVersions": "/^[0-9]+\\.[0-9]+\\.[0-9]+$/"
    }
  ]
}
